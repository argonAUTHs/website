    padding: <%= Number(cards.CARD_3.GAP.phone.y) - Number(cards.CARD_3.BORDER) %>px <%= Number(cards.CARD_3.GAP.phone.x) - Number(cards.CARD_3.BORDER) %>px;
  }
}

@media (min-width: <%= responsive.MEDIA.tablet %>px) {
  .card-3 {
    padding: <%= Number(cards.CARD_3.GAP.tablet.y) - Number(cards.CARD_3.BORDER) %>px <%= Number(cards.CARD_3.GAP.tablet.x) - Number(cards.CARD_3.BORDER) %>px;
  }
}

@media (min-width: <%= responsive.MEDIA.laptop %>px) {
  .card-3 {
    padding: <%= Number(cards.CARD_3.GAP.laptop.y) - Number(cards.CARD_3.BORDER) %>px <%= Number(cards.CARD_3.GAP.laptop.x) - Number(cards.CARD_3.BORDER) %>px;
  }
}

@media (min-width: <%= responsive.MEDIA.desktop %>px) {
  .card-3 {
    padding: <%= Number(cards.CARD_3.GAP.desktop.y) - Number(cards.CARD_3.BORDER) %>px <%= Number(cards.CARD_3.GAP.desktop.x) - Number(cards.CARD_3.BORDER) %>px;
  }
}


@media (max-width: <%= responsive.MEDIA.phone - 1 %>px) {
  .card-3 .spacer-48 {
    padding-top: 0;
