    background-size: auto 350px;
    background-position: bottom 18% right 0;
  }
}

@media (max-width: <%= responsive.MEDIA.phone - 1 %>px) {
  .arrow-link-small img{
    margin-top: 0;
    width: 70px;
  }

  .timeline li::after{
    top: 5px;
  }

  .timeline li::before{
    height: calc(100% - 36px);
    top: 43px;
  }

  .arrow-link img{
    width: 70px;
  }

  .arrow-link-type2 img {
    margin-top: 15px;
    width: 80px;
  }
}

.card-3 {
  border: <%= Number(cards.CARD_3.BORDER) %>px solid <%= colors["Primary"]["500"].color %>;
  padding: <%= Number(cards.CARD_3.GAP.default.y) - Number(cards.CARD_3.BORDER) %>px <%= Number(cards.CARD_3.GAP.default.x) - Number(cards.CARD_3.BORDER) %>px;
}

