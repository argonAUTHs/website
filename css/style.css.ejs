}


.systems__content-block img {
  position: absolute;
  width: 100%;
  height: auto;
  top: 0;
  left: 0;
}

@supports (width: calc(100% + 80px)) {
  .systems__content-block img {
    width: calc(100% + 80px);
  }

  @media(max-width: <%= responsive.MEDIA.phone - 1 %>px) {
    .systems__content-block img {
      width: calc(100% + 12px);
    }
  }
}

