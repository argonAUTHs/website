---
  permalink: css/style.css
---

.bg-container {
  position: relative;
  max-width: <%= responsive.MAX_WIDTH %>px;
  margin: 0 auto;
}


html {
  scroll-behavior: smooth;
}

body {
  color : <%= colors['On Background'].color %>;
  background-color: <%= colors['Background'].color %>;
  margin : 0;
  padding : 0;
}

ul {
  padding: 0;
  margin: 0;
  list-style: none;
}

img {
  width: 100%;
  height: auto;
}

a img {
  border: 0;
}

.primary-500 {
  color: <%= colors["Primary"]["500"].color %>;
}

.text-uppercase {
  text-transform: uppercase;
}

.text-center {
  text-align: center;
}

.list-reset {
  padding-left: 0;
  margin: 0;
}

.time{
  color: <%= colors["Primary"]["500"].color %>;
}

.strong{
  font-weight: 700;
}

.timeline li {
  position: relative;
  padding-left: 80px;
}

.timeline li::after{
  content: "";
  width: 33px;
  height: 33px;
  border: 3px solid <%= colors["Primary"]["500"].color %>;
  position: absolute;
  border-radius: 50%;
  left: 22px;
  top: 20px;  
}

.timeline li::before{
  content: "";
  width: 3px;
  height: calc(100% - 39px);
  background: <%= colors["Primary"]["500"].color %>;
  position: absolute;
  left: 40px;
  top: 59px;
}

.timeline-exception{
  margin-left: 45px;
}

.timeline-item-exception::before{
  display: none;
}

.story-arrows{
  margin-left: 246px;
}

@media (min-width: <%= responsive.MEDIA.tablet %>px) {
  .text-center-lp {
    text-align: center;
  }
}

.ml-auto {
  margin-left: auto;
}

.hero {
  background-image: url("../img/backgrounds/_home-bkg.png");
  background-position: center right 65%;
  background-size: contain;
  background-repeat: no-repeat;
}

.nowrap{
  flex-wrap: nowrap;
}

@media (max-width: <%= responsive.MEDIA.laptop - 1 %>px) {
  .story-arrows{
    margin-left: 125px;
  }
}

@media (max-width: <%= responsive.MEDIA.tablet - 1 %>px) {
  .hero {
    background-image: url(../img/backgrounds/_home-2-bkg.png);
    background-size: cover;
    background-position: top -80px center;
    background-repeat: no-repeat;
  }

  .story-arrows{
    margin: 0;
    margin-left: auto;
    padding-right: <%= responsive.GAP %>px;
  }

  .timeline-exception{
    margin-left: 22px;
    margin-top: -2px;
  }

  .timeline-exception{
    margin-left: 0;
  }
}

.our-story {
  background-image: url("../img/backgrounds/_about-bkg.png");
  background-position: top 0 left 0;
  background-size: contain;
  background-repeat: no-repeat;
}

@media (max-width: <%= responsive.MEDIA.tablet - 1 %>px) {
  .our-story {
    background-size: cover;
  }
}

@media (max-width: <%= responsive.MEDIA.phone - 1 %>px) {
  .our-story {
    background-size: cover;
    background-position: top 0 left 15%;
  }
}

.our-domain {
  background-image: url("../img/backgrounds/circles.png");
  background-position: top 24% right 0;
  background-size: auto 80%;
  background-repeat: no-repeat;
}

@media (max-width: <%= responsive.MEDIA.tablet - 1 %>px) {
  .our-domain {
    background-size: auto 70%;
    background-position: bottom 18% right 0;
  }
}

@media (max-width: <%= responsive.MEDIA.phone - 1 %>px) {
